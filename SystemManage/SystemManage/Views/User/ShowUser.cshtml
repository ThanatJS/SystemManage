@model List<SystemManage.Models.UserModel>
@using SystemManage.Models;

@{
    ViewBag.Title = "ShowUser";
}
<div class="container">
    <table class="tabel table-bordered">
        <thead>
            <tr>
                <th>No</th>
                <th>User_ID</th>
                <th>User_Email</th>
                <th>User_Password</th>
                <th>User_Name</th>
                <th>User_LastName</th>
                <th>Address</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
            @{ 
                var i = 1;
                foreach (var item in (List<UserModel>)ViewBag.DataList)
                {
                    <tr>
                        <td>@i</td>
                        <td>@item.Users_ID</td>
                        <td>@item.User_Email</td>
                        <td>@item.User_Password</td>
                        <td>@item.User_Name</td>
                        <td>@item.User_LastName</td>
                        <td>@item.Address</td>    
                    </tr>
                    i++;
                }



            }

        </tbody>
    </table>
</div>
